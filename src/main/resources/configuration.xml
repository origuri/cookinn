<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <settings>
        <setting name="mapUnderscoreToCamelCase" value="true"/>
    </settings>
    <typeAliases>
        <!--member-->
        <typeAlias alias="MemberDto" type="com.example.cookinn.model.member.MemberDto"/>
        <typeAlias alias="MemberUpdateDto" type="com.example.cookinn.model.member.MemberUpdateDto"/>

        <!--store-->
        <typeAlias alias="StoreDto" type="com.example.cookinn.model.store.StoreDto"/>
        <typeAlias alias="StoreUpdateDto" type="com.example.cookinn.model.store.StoreUpdateDto"/>

        <!--item-->
        <typeAlias alias="ItemDto" type="com.example.cookinn.model.item.ItemDto"/>
        <typeAlias alias="ItemSearchDto" type="com.example.cookinn.model.item.ItemSearchDto"/>
        <typeAlias alias="ItemUpdateDto" type="com.example.cookinn.model.item.ItemUpdateDto"/>

        <!--category-->
        <typeAlias alias="categoryLargeDto" type="com.example.cookinn.model.category.CategoryLargeDto"/>
        <typeAlias alias="CategoryMidDto" type="com.example.cookinn.model.category.CategoryMidDto"/>

        <!--cart-->
        <typeAlias alias="CartInsertDto" type="com.example.cookinn.model.cart.CartInsertDto"/>
        <typeAlias alias="CartDto" type="com.example.cookinn.model.cart.CartDto"/>
        <typeAlias alias="CartUpdateDto" type="com.example.cookinn.model.cart.CartUpdateDto"/>
    </typeAliases>
</configuration>